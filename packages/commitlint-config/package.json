{
  "name": "@canideploy/commitlint-config",
  "version": "0.0.0",
  "description": "commitlint config using conventional commits",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "commitlint",
    "commitlint-config",
    "conventional-changelog"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canideploy/configs.git",
    "directory": "packages/commitlint-config"
  },
  "author": "Eric King <eric@canideploy.dev> (https://canideploy.dev)",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/commitlint-config.js",
  "module": "./dist/commitlint-config.js",
  "types": "./dist/commitlint-config.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/commitlint-config.d.ts",
      "@canideploy/configs-monorepo": "./src/commitlint-config.ts",
      "import": "./dist/commitlint-config.js",
      "default": "./dist/commitlint-config.js"
    }
  },
  "dependencies": {
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/types": "catalog:",
    "commitlint-plugin-cspell": "^0.6.0"
  },
  "devDependencies": {
    "@commitlint/lint": "^20.4.2",
    "@commitlint/load": "^20.4.0",
    "commitlint": "catalog:",
    "conventional-commits-parser": "^6.2.1"
  },
  "peerDependencies": {
    "commitlint": ">=20"
  }
}
