{
  "name": "@canideploy/prettier-config",
  "version": "0.0.0",
  "description": "shared prettier config",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "prettier"
  ],
  "homepage": "https://github.com/canideploy/configs/#readme",
  "bugs": {
    "url": "https://github.com/canideploy/configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canideploy/configs.git",
    "directory": "packages/prettier-config"
  },
  "author": "Eric King <eric@canideploy.dev> (https://canideploy.dev)",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/prettier.config.js",
  "module": "./dist/prettier.config.js",
  "types": "./dist/prettier.config.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/prettier.config.d.ts",
      "@canideploy/configs-monorepo": "./src/prettier.config.ts",
      "import": "./dist/prettier.config.js",
      "default": "./dist/prettier.config.js"
    }
  },
  "dependencies": {
    "prettier-plugin-packagejson": "catalog:"
  },
  "devDependencies": {
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "prettier": ">=3"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
