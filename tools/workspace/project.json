{
  "name": "workspace",
  "projectType": "library",
  "root": "tools/workspace",
  "sourceRoot": ".",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["*.ts", "*.cts", "*.mts", "*.js", "*.cjs", "*.mjs"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tools/workspace/tsconfig.json --noEmit"
      }
    }
  }
}
